{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "preview",
          "name": "preview",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.content.pageFolders": [
    {
      "title": "research",
      "path": "[[workspace]]/research"
    },
    {
      "title": "about",
      "path": "[[workspace]]/about"
    },
    {
      "title": "essay",
      "path": "[[workspace]]/essay"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/_posts"
    }
  ],
  "frontMatter.taxonomy.tags": [],
  "frontMatter.taxonomy.categories": [],
  "frontMatter.dashboard.openOnStart": true
}